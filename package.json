{
  "name": "ng-app",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "bundle": "browserify -s main dist/app/main.js > app/bundle.js  && npm run minify-bundle",
    "minify-bundle": "uglifyjs app/bundle.js --screw-ie8 --compress --mangle --output app/bundle.min.js",
    "api": "node server/api-server.js",
    "api-with-auth": "node server/api-server.js authEnabled",
    "api-nocors": "node server/api-server.js noCors",
    "build": "gulp copy-libs",
    "build-copy": "gulp copy:assets",
    "start-wp": "webpack-dev-server --inline --progress --port 8080",
    "build-wp": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "test-wp": "karma start karma-wp.config.js"
  },
  "keywords": [],
  "author": "",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",

    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4",
     

    "express": "^4.14.0",
    "json-server": "^0.8.14",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.16.2",
    "jquery": "^2.2.4",
    "moment": "^2.15.1"
  },
  "devDependencies": {

    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.0.10",

    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.15.1",
    "lodash": "^4.16.4",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",

    "@types/node": "^6.0.46",
    "@types/jasmine": "^2.5.36",
    "@types/jquery": "^2.0.34",
    "@types/lodash": "^4.14",

    "browserify": "^13.1.0",
    "uglifyjs": "^2.4.10",
    "gulp": "^3.9.1",

    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    
    "css-loader": "^0.26.0",
    "enhanced-resolve": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",

    "karma-htmlfile-reporter": "^0.3.4",
     
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
     
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    
    "style-loader": "^0.13.1",
     
